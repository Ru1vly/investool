# Google Gemini API Configuration
GOOGLE_API_KEY=your_gemini_api_key_here

# Model Configuration
GEMINI_PRO_MODEL=gemini-1.5-pro-latest
GEMINI_FLASH_MODEL=gemini-1.5-flash-latest

# Database Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=finrisk_db
POSTGRES_USER=finrisk_user
POSTGRES_PASSWORD=your_password_here

# Redis Configuration (for KV caching)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# Neo4j Configuration (for GraphRAG)
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_neo4j_password

# Memory System
MEM0_ENABLED=true
MEM0_GRAPH_ENABLED=true

# Production Settings
ENVIRONMENT=development
LOG_LEVEL=INFO
MAX_RETRIES=3
TIMEOUT_SECONDS=120
